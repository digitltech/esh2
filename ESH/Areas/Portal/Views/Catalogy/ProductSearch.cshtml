@model IEnumerable<ESH.Models.Product>

    <div id="searchresult">
        @if (Model.Count() > 0)
        {
            <ul>
                @foreach(var item in Model)
                {
                    @using (Ajax.BeginForm("Join", "Catalogy", new AjaxOptions { UpdateTargetId = "results" }))
                    {
                        <li>@item.Article @item.Name <input type="submit" value="Выбрать"> </li>
                        <li id="results"></li>
                    }

                }
            </ul>
        }
    </div>
